{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build-normal",
        "type": "shell",
        "command": "${workspaceRoot}/pawno/bin/pawncc.exe",
        "args": ["'${fileBasename}'", "'-D${fileDirname}'", "'-;+'", "'-(+'", "'-d3'"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "isBackground": false,
        "problemMatcher": "$pawncc"
      }
    ]
}